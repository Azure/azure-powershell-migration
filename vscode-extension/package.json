{
	"name": "azure-powershell-migration",
	"displayName": "Azure PowerShell Migration",
	"description": "TBD",
	"version": "0.0.1",
	"publisher": "azure-powershell",
	"engines": {
		"vscode": "^1.26.0"
	},
	"categories": [
		"Azure"
	],
	"activationEvents": [
		"onCommand:azure-powershell-migration.selectVersion"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"title": "Migrate Azure PowerShell Script",
				"command": "azure-powershell-migration.selectVersion"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
